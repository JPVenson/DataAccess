<Window x:Class="JPB.DataAccess.EntityCreator.UI.MsSQL.View.ClassPreviewWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:JPB.DataAccess.EntityCreator.UI.MsSQL.View"
		xmlns:avalonedit="http://icsharpcode.net/sharpdevelop/avalonedit"
		xmlns:viewModel="clr-namespace:JPB.DataAccess.EntityCreator.UI.MsSQL.ViewModel"
		mc:Ignorable="d"
		Title="ClassPreviewWindow" Height="300" Width="300" d:DataContext="{d:DesignInstance viewModel:ClassPreviewViewModel}">
	<DockPanel>
		<local:BindableTextEditor Text="{Binding Result}" SyntaxHighlighting="{Binding HightlightProvider}"></local:BindableTextEditor>
	</DockPanel>
</Window>
